{
  "manifest_version": 3,
  "name": "Dcard Damare",
  "version": "1.0",
  "description": "Stop Dcard popup login window annoying reading",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://www.dcard.tw/*"
      ]
    }
  ]
}
